<script type="text/javascript">
    var width = 0;
    var timer;
    timer = setInterval(function() {
        width += 1;
        if (width >= 200) {
            clearInterval(timer);
            $('.progressBox').css("display","none");
            $('.btnList').css("display","block")
        }
        document.getElementsByClassName("first")[0].style.width = width + "px";

        document.getElementById("running").style.left = width + "px";

    }, 10);
    
    var val= $('#screen').val();

    function w_ht(){
        $('.detailCenter').addClass('act');
        $('.detailLeft').addClass('act');
        $('.detailRight').addClass('act');
    }

    if( /Android|webOS|iPhone|iPad|iPod|BlackBerry/i.test(navigator.userAgent) ) {
    }else {
        if (val ==0){
            w_ht();
            $('#iframeGame').addClass('actq');
            window.addEventListener("resize", response_size);
            response_size();
            function response_size() {
                var app_iframe = document.getElementById('iframeGame')
                var w_c = 9 / 16 * app_iframe.offsetHeight;
                app_iframe.style.width = w_c + 'px';
            }
        }
    };

    var iframeValue= $('#iframeValue').val();

    load = true;

    function iframe_load (){
        $('.detailContBox').css('display','none');
        $('.btnMenu').css('display','flex');
        if(load){
            $('#iframeGame').attr('src',iframeValue)
        }
        load =false
    }

    

    $('.btnMenu').click(function () {
        $('.detailContBox').css('display','flex')
    });

    $('.open-detail').click(function () {
        $('.detailContBox').css('display','flex')
    });


    var language = localStorage.getItem("language");

    function isLike(){
        $('.num').css('display','none');
        if (language == "en"){
            $('.like').html('Thanks +1');
            $('.tips').html('Thanks for your like!');
        }else {
            $('.like').html('喜欢 +1');
            $('.tips').html('感谢您的喜欢!');
        }
    }

    // 单次点赞判断
    var zanId = $('#game_id').val();
    var zan_ids = localStorage.getItem("zan-ids");
    zan_ids = zan_ids ? zan_ids.split(",") : [];
    if (zan_ids.indexOf(zanId)!==-1){
        $('.islike').addClass("act");
        isLike();
    }

    function addZan(obj) {
        if (zan_ids.indexOf(zanId)!==-1){
            return
        }
        // 点赞
        num = 1;
        obj.classList.add("act");
        isLike();
        $.ajax({
            url: "/interest/api/set_addzan",
            method: "post",
            data: {
                num: num,
                id: 2242            },
            success: function (result) {
                // 点赞记录游戏
                zan_ids.unshift(zanId);
                zan_ids = [...new Set(zan_ids)];
                localStorage.setItem("zan-ids", zan_ids);
            },
            error:function (){
            }
        });
    }

    var preview = new Swiper('.preview', {
        freeMode: true,
        slidesPerView: 'auto',
        spaceBetween: 8,
        watchSlidesVisibility: true,
        observer: true,
        observeSlideChildren: true,
        observeParents: true,
    });


    var more = document.querySelectorAll('.moreShow');
    more.forEach(function (el) {
        el.addEventListener("click", function () {
            if (!this.parentNode.previousElementSibling.classList.contains("showAll")){
                this.parentNode.previousElementSibling.classList.add("showAll");
                this.innerHTML='Read Less';
                language == "en"? this.innerHTML='Read Less': this.innerHTML='阅读更少';
            }else {
                this.parentNode.previousElementSibling.classList.remove("showAll");
                language == "en"? this.innerHTML='Read More': this.innerHTML='阅读更多';

            }
        });
    });

    var gameId = $('#game_id').val();
    var game_ids = localStorage.getItem("games-ids");
    game_ids = game_ids ? game_ids.split(",") : [];
    game_ids.unshift(gameId);
    game_ids = [...new Set(game_ids)];
    localStorage.setItem("games-ids", game_ids.slice(0,12).join(','));


    var collect_ids = localStorage.getItem("collect-ids");

    var collectId = $('#collect_id').val();
    collect_ids = collect_ids ? collect_ids.split(",") : [];

    $(document).ready(function () {
        collect_ids.includes(collectId)?$('.collectBtn').addClass('act'):'';
    });

    $('.collectBtn').click(function () {
        if (!collect_ids.includes(collectId)){
            
            $('.collectBtn').addClass('act');
            // 
            collect_ids.unshift(collectId);
            // 
            collect_ids = [...new Set(collect_ids)];
            localStorage.setItem("collect-ids", collect_ids.join(','));
        } else {
            $('.collectBtn').removeClass('act');
            collect_ids.unshift(collectId);
            collect_ids = [...new Set(collect_ids)];
            // 
            collect_ids.shift(collectId);
            localStorage.setItem("collect-ids", collect_ids.join(','));
        }
    });

    if($('.video-section').length>0 && $('.video-section').children('iframe').length>0){
        var player = Stream( document.getElementsByClassName('video-section')[0].getElementsByTagName("iframe")[0]);
    }
    $('.close-detail').click(function () {
        iframe_load()
        if(player){
            player.pause();
        }
    });
    $('.play').click(function () {
        iframe_load()
        if(player){
            player.pause();
        }
    });
</script>
