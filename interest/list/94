<script>
    if( /Android|webOS|iPhone|iPad|iPod|BlackBerry/i.test(navigator.userAgent) ) {
        $('.adList').addClass('act')
    }
    
    var ids = localStorage.getItem("games-ids");

    function sortdata(data1,data2) {
        return data1.map(n=>{return data2.filter(nn=>nn.gameId == n)[0]}).filter(n=>n);
    }

    if(!ids){
        res='';
        $('#historyBox').empty();
    }else{
        $.ajax({
                type: 'get',
                url: '/interest/api/infos?ids='+ids,
                dataType:'jsonp',
                processData: false,
                jsonp: "callback",
                jsonpCallback:"jsonpCallback",
                cache: true,
                success:function (result) {
                    if(result.code == 1) {
                        var res = "";
                        sortdata(ids.split(','),result.data).forEach(result=>{
                            res+=`
                     <li class="swiper-slide">
                         <a href="/interest/detail/${result.classId}/${result.gameId}">
                             <img src="/gamebox/img/lazy.png" class="lazyLoad" data-src="${result.icon}" alt="">
                         </a>
                     </li>

                    `;
                        });
                        $('#history').append(res);
                        $("img.lazyLoad").lazyload();
                    }
                }
            }
        )
    }

    // $('#notice').html("<i class=\"iconfont icon-loader-4-line\"></i>Loading...");
    // function throttle(fn,delay){
    //     var timer;
    //     return function (){
    //         var _this = this;
    //         var arg = arguments;
    //         if(timer){
    //             return ;
    //         }
    //         timer = setTimeout(function (){
    //             fn.apply(_this,arg);
    //             timer = null;
    //         },delay);
    //     }
    // }
    var throttleFn = throttle(scrollEvent,1000);

    var isloading = true;

    var page = 2;
    function scrollEvent(){
        // 滚动区域到头部的距离
        var scrollTop = $(this).scrollTop();
        // 可滚动的高度
        var scrollHeight = $(document).height();
        // 屏幕的高度
        var windowHeight = $(this).height();
        if (scrollTop + windowHeight  + 80 >= scrollHeight && isloading) {
            scrollAjax();
        }
    }

    function scrollAjax (){
        $.ajax({
            type: 'GET',
            url: "/interest/more/94/"+page+'/24',
            dataType: 'json',
            success: function (result) {
                // var result = JSON.parse(result);
                var res = "";
                var resCont = "";
                var resAd = "";
                var pageCount = typeof(result)!= 'undefined' ? result.length : 0;
                if (result.length >= 1){
                    if (pageCount > 0) { //pageCount > 0
                        isloading = true;
                        $.each(result, function (i, item) {
                            // 游戏图标循环
                            resCont+=`
                            <li>
                             <a href="/interest/detail/${item.classId}/${item.gameId}">
                                 <div class="thumb">
                                     <img src="/gamebox/img/lazy.png" class="lazyLoad" data-src="${item.icon}" alt="">
                                 </div>
                                 <dl>
                                     <dt>
                                        ${item.name}
                                     </dt>
                                     <dd>
                                         <span>${item.playtime}</span>
                                         Player
                                     </dd>
                                 </dl>
                             </a>
                         </li>`
                        });
                        // 整体模块渲染
                        res=
                            `<ul class="listBox">`
                            + resCont+ `</ul>`;
                        $('#gameCont').append(res);
                        page++;
                    }else {
                        $('#notice').html(" <span style='color: rgba(0, 0, 0, 0.45)'>That's all :)</span>")
                        isloading = false;
                    }
                }else {
                    $('#notice').html(" <span style='color: rgba(0, 0, 0, 0.45)'>That's all :)</span>")
                    isloading = false;
                }
                $("img.lazyLoad").lazyload();
            },
            error:function (result) {
                $('#notice').html("<i class=\"iconfont icon-error-warning-line\"></i>Loading...")
            }
        });
    }

    if (document.body.clientHeight > 1080){
        scrollAjax()
    }

    $(window).scroll(function (){
        throttleFn();
    });

</script>

